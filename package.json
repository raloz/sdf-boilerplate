{
	"name": "noname",
	"version": "1.0.0",
	"description": "Awesome sdf account customization project",
	"scripts": {
		"postinstall": "husky install && node .paw/post-install.js",
		"account:auth": "node .paw/auth-to-account.js"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"author": "@Octavio_Quiroz",
	"license": "ISC",
	"bugs": {},
	"homepage": "",
	"dependencies": {
		"dotenv": "^16.0.0"
	},
	"devDependencies": {
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"prettier": "^2.6.0"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"amd": true,
			"es2021": true
		},
		"extends": [
			"prettier"
		],
		"plugins": [
			"prettier"
		],
		"parserOptions": {
			"ecmaVersion": "latest"
		},
		"rules": {
			"object-curly-spacing": [
				2,
				"always"
			],
			"object-property-newline": [
				"error",
				{
					"allowAllPropertiesOnSameLine": false
				}
			],
			"object-curly-newline": [
				"warn",
				{
					"ObjectExpression": "always",
					"ObjectPattern": {
						"multiline": true
					},
					"ImportDeclaration": "never",
					"ExportDeclaration": {
						"multiline": true,
						"minProperties": 0
					}
				}
			],
			"indent": [
				"warn",
				4
			],
			"linebreak-style": [
				"warn",
				"unix"
			],
			"quotes": [
				"warn",
				"double"
			],
			"semi": [
				"warn",
				"always"
			]
		}
	}
}